<mujoco model="equality_invalid_names">
  <option viscosity="1">
    <flag contact="disable"/>
  </option>

  <default>
    <default class="box">
      <geom type="box" size=".2" fromto=".2 .2 0 .2 .2 -1" rgba=".4 .7 .6 1"/>
    </default>
    <site rgba=".0 .7 0 1" size="0.05"/>
  </default>

  <worldbody>
    <geom pos="0 0 -2" type="plane" size="10 10 .01"/>
    <site name="a1" pos="-2 0 0" euler="-90 0 0"/>
    <body name="a" pos="-2 1 0">
      <freejoint/>
      <geom class="box"/>
      <site name="a2" pos="0 -1 0"/>
      <body pos="0 0 -1">
        <geom class="box" size=".15"/>
      </body>
    </body>

    <site name="b1" euler="-90 0 0"/>
    <body name="b" pos="0 1 0">
      <freejoint/>
      <geom class="box"/>
      <site name="b2" pos="0 -1 0"/>
      <body pos="0 0 -1">
        <geom class="box" size=".15"/>
      </body>
    </body>
  </worldbody>

  <equality>
    <!-- equivalent connect and weld constraints defined with sites (enabled) and bodies (disabled) -->
    <connect name="connect_site" site1="a1"    site2="a2"    active="true"/>
    <connect name="connect_body" body1="a"     body2="world" active="false" anchor="0 -1 0"/>
    <weld    name="weld_site"    site1="b1"    site2="b2"    active="true"/>
    <weld    name="weld_body"    body1="world" body2="b"      active="false" anchor="0 0 0" relpose="0 -1 0 0.70710678 0.70710678 0 0"/>
  </equality>
</mujoco>
